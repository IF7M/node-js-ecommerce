<%- include('./partials/adminheader') %>
<main>
<div class="container mt-2">
<h2 class="page-title">اضافة تصنيف</h2>
<a href="/admin/categories" class="btn btn-primary mt-2">الرجوع الى التصنيفات</a>
</div>

<div class="container">
 <form action="/admin/categories/add-category" method="post">
<div class="form-group">
    <label for="">العنوان</label>
    <input type="text" class="form-control" name="title" value="<%= title %>" placeholder="Title">
</div>
<div class="form-group">
    <label for="">الرابط</label>
    <input type="text" class="form-control" name="slug" value="<%= slug %>" placeholder="Slug">
</div>


<div class="form-group mt-2">
<button class="btn btn-primary" >اضافة</button>
</div>
 </form>


</div>
</main>




















<%- include('./partials/adminfooter') %>